{
  "name": "gabriel.siedler.ninja",
  "version": "1.0.0",
  "description": "Static portfolio page",
  "main": "./index.js",
  "scripts": {
    "start": "node .",
    "start-dev": "nodemon .",
    "test": "eslint . && stylelint \"app/sass/*.scss\" --syntax scss",
    "build-sass": "sass --sourcemap=none ./app/sass/all.scss ./app/public/css/app.min.css --style compressed",
    "watch-sass": "sass --sourcemap=none --watch ./app/sass/all.scss:./app/public/css/app.min.css --style compressed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielsiedler/gabriel.siedler.ninja.git"
  },
  "author": "Gabriel Siedler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielsiedler/gabriel.siedler.ninja/issues"
  },
  "homepage": "http://gabriel.siedler.ninja",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.12.4",
    "font-awesome": "^4.6.3",
    "jquery": "^2.2.4",
    "moment": "^2.11.2",
    "request-promise": "^2.0.1",
    "slick-carousel": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.14.0",
    "nodemon": "^1.10.0",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0"
  },
  "engines": {
    "node": "6.2.2"
  }
}
